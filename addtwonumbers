using System;

namespace addtwonumbers
{
    class Program
    {
        static void Main(string[] args)
        {
            string s = Console.ReadLine();
            string[] values = s.Split(" ");
            int a = int.Parse(values[0]);
            int b = int.Parse(values[1]);
            int c = a + b;

            Console.WriteLine(c);
        }
    }
}
